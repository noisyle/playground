<!--pages/playground5/index.wxml-->
<navigation-bar back="{{true}}" home="{{true}}" title="{{date}}"></navigation-bar>
<image class="bg" src='/images/bg_full.jpg' mode='aspectFill' />
<view class="container" style="padding-top: {{navBarHeight}}px;">
  <button bindtap="registerOrLogin">登录或注册</button>
  <button bindtap="me">显示信息</button>
  <button bindtap="getUserProfile">获取头像昵称</button>
  <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber">获取手机号</button>
  <button bindtap="compressVideo">视频压缩上传</button>
</view>
